{
    "presets": [
        [ "es2015", { "modules": false } ],
        "react",
        "stage-0",
        "react-hmre"
    ],
    "plugins": [
        ["module-alias", [
            { "src": "./src/components", "expose": "components" },
            { "src": "./src/styles", "expose": "styles" },
            { "src": "./src/reducers", "expose": "reducers" },
            { "src": "./src/actions", "expose": "actions" },
            { "src": "./src/sagas", "expose": "sagas" },
            { "src": "./src/store", "expose": "store" },
            { "src": "./src/assets", "expose": "assets" },
            { "src": "./src/assets/images", "expose": "images" }
        ]
    ]
  ]
}